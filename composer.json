{
	"name": "kwattro/fathom",
    "type": "library",
    "description": "Lightweight PHP Neo4J OGM",
    "keywords": ["neo4j", "ogm", "php"],
    "time": "2013-06-22",
    "license": "MIT",
	"require": {
		"symfony/yaml": "dev-master",
		"symfony/console": "dev-master",
		"symfony/finder": "dev-master",
		"monolog/monolog": "~1.3",
		"kriswallsmith/buzz": "dev-master",
		"guzzle/guzzle": "dev-master",
		"lootils/uuid": "dev-master",
		"cedriclombardot/twig-generator": "dev-master"
	},
	"require-dev":{
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-0": {
			"Neoxygen": "src/",
			"Fathom": "src/"
		}
	},
	"minimum-stability": "dev",
	"bin": ["bin/fathom"]
}
